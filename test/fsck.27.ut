## -*- shell-script -*-

TOOL=ute
OUTFILE=`mktemp "/tmp/tmp.XXXXXXXX"`
COMMENT="testing ex situ conversions"
CMDFILE=`mktemp "/tmp/tmp.XXXXXXXX"`

if test "${endian}" = "big"; then
	INFILE="${srcdir}/mux.7.ref.ute"
	REFFILE="${srcdir}/mux.7.ref.beute"
elif test "${endian}" = "little"; then
	INFILE="${srcdir}/mux.7.ref.beute"
	REFFILE="${srcdir}/mux.7.ref.ute"
fi

cat <<EOF > "${CMDFILE}"
cp '${INFILE}' '${OUTFILE}'
"\${TOOL}" fsck '${OUTFILE}'
EOF

