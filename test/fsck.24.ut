## -*- shell-script -*-

TOOL=ute
INFILE="${srcdir}/fsck.17.ref.leute"
OUTFILE=$(mktemp "/tmp/tmp.XXXXXXXX")
COMMENT="testing decompression/compression"
CMDFILE=$(mktemp "/tmp/tmp.XXXXXXXX")

cat <<EOF > "${CMDFILE}"
cp -a '${INFILE}' '${OUTFILE}'
"\${TOOL}" fsck --decompress '${OUTFILE}'
"\${TOOL}" fsck --compress '${OUTFILE}'
EOF

