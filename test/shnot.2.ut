## -*- shell-script -*-

TOOL=ute
INFILE="${srcdir}/print.d.ute"
CMDFILE="${TS_TMPDIR}/cmdfile"

cat <<EOF > "${CMDFILE}"
TMPIN='${INFILE}'
TMPOUT='${TS_OUTFILE}'
if test "${endian}" = "big"; then
	TMPIN=\`mktemp "/tmp/tmp.XXXXXXXX"\`
	TMPOUT=\`mktemp "/tmp/tmp.XXXXXXXX"\`
	"\${TOOL}" fsck --big-endian '${INFILE}' -o "\${TMPIN}"
fi
"\${TOOL}" shnot -i 1 -o "\${TMPOUT}" "\${TMPIN}"
if test "${endian}" = "big"; then
	"\${TOOL}" fsck --little-endian "\${TMPOUT}" -o '${TS_OUTFILE}'
	rm -f -- "\${TMPIN}" "\${TMPOUT}"
fi
EOF

REFFILE="${srcdir}/shnot.2.ref.ute"

## shnot.2.ut ends here
