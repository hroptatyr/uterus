## -*- shell-script -*-

TOOL=ute
COMMENT="testing in situ compression of compressed data"
CMDFILE=`mktemp "/tmp/tmp.XXXXXXXX"`

if test "${endian}" = "big"; then
	INFILE="${srcdir}/fsck.17.ref.beute"
	REFFILE="${srcdir}/fsck.17.ref.beute"
elif test "${endian}" = "little"; then
	INFILE="${srcdir}/fsck.17.ref.leute"
	REFFILE="${srcdir}/fsck.17.ref.leute"
fi

cat <<EOF > "${CMDFILE}"
cp '${INFILE}' '${TS_OUTFILE}'
"\${TOOL}" fsck --compress '${TS_OUTFILE}'
EOF

## fsck.20.ut ends here
