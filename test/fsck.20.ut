## -*- shell-script -*-

TOOL=ute
INFILE="${srcdir}/fsck.17.ref.ute"
OUTFILE=$(mktemp "/tmp/tmp.XXXXXXXX")
COMMENT="testing in situ compression of compressed data"
CMDFILE=$(mktemp "/tmp/tmp.XXXXXXXX")

cat <<EOF > "${CMDFILE}"
cp -d '${INFILE}' '${OUTFILE}'
"\${TOOL}" fsck --compress '${OUTFILE}'
EOF

## outfile reference, hopefully this is portable
REFFILE="fsck.17.ref.ute"

