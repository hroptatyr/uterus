## -*- shell-script -*-

TOOL=ute
COMMENT="testing ex situ conversions"
CMDFILE="${TS_TMPDIR}/cmdfile"

if test "${endian}" = "big"; then
	INFILE="${srcdir}/mux.4.ref.ute"
	REFFILE="${srcdir}/mux.4.ref.beute"
elif test "${endian}" = "little"; then
	INFILE="${srcdir}/mux.4.ref.beute"
	REFFILE="${srcdir}/mux.4.ref.ute"
fi

cat <<EOF > "${CMDFILE}"
cp '${INFILE}' '${TS_OUTFILE}'
chmod u+w '${TS_OUTFILE}'
"\${TOOL}" fsck '${TS_OUTFILE}'
EOF

