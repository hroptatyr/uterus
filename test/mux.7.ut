## -*- shell-script -*-

TOOL=ute
INFILE="${srcdir}/mux.f.ibhist.xml"
CMDFILE="${TS_TMPDIR}/cmdfile"

cat <<EOF > "${CMDFILE}"
"\${TOOL}" mux -f ibhist '${INFILE}' -o '${TS_OUTFILE}'
if test "${endian}" = "big"; then
	"\${TOOL}" fsck --little-endian '${TS_OUTFILE}'
fi
EOF

