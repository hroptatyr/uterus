## -*- shell-script -*-

TOOL=ute
INFILE="${srcdir}/mux.h.dukas.bi5"
OUTFILE="testload.ute"
CMDFILE=$(mktemp "/tmp/tmp.XXXXXXXX")

endian()
{
	ind=$(echo -n I | od -to2 | head -n1 | cut -f2 -d" " | cut -c6)
	if test "${ind}" = "0"; then
		echo "big"
	elif test "${ind}" = "1"; then
		echo "little"
	else
		echo "unknown"
	fi
}

cat <<EOF > "${CMDFILE}"
TMPOUT='${OUTFILE}'
"\${TOOL}" mux -f dukas --refdate 1328475600 --name 'AUDUSD' '${INFILE}' -o "\${TMPOUT}"
if test "$(endian)" = "big"; then
	"\${TOOL}" fsck --little-endian "\${TMPOUT}" -o '${OUTFILE}'
	rm -f -- "\${TMPOUT}"
fi
EOF

