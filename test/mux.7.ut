TOOL=ute
INFILE="${srcdir}/mux.f.ibhist.xml"
OUTFILE="testload.ute"
CMDFILE=`mktemp "/tmp/tmp.XXXXXXXX"`

cat <<EOF > "${CMDFILE}"
"\${TOOL}" mux -f ibhist '${INFILE}' -o '${OUTFILE}'
if test "${endian}" = "big"; then
	"\${TOOL}" fsck --little-endian '${OUTFILE}'
fi
EOF

