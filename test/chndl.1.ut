## -*- shell-script -*-

TOOL=ute
INFILE="${srcdir}/mux.7.ref.ute"
OUTFILE="testload.ute"
COMMENT="half-hour candles cannot be refined"
CMDFILE=$(mktemp "/tmp/tmp.XXXXXXXX")

cat <<EOF > "${CMDFILE}"
TMPIN='${INFILE}'
if test "${endian}" = "big"; then
	TMPIN=\$(mktemp "/tmp/tmp.XXXXXXXX")
	"\${TOOL}" fsck --big-endian -o "\${TMPIN}" '${INFILE}'
fi
"\${TOOL}" chndl -i 60 -o '${OUTFILE}' "\${TMPIN}"
if test "${endian}" = "big"; then
	"\${TOOL}" fsck --little-endian '${OUTFILE}'
fi
EOF

OUTFILE_SHA1="1b9ab07823cfbf289060c0779f67fd8ea51d959d"

